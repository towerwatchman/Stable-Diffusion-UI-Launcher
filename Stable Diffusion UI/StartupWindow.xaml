<Window x:Class="Stable_Diffusion_UI.StartupWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Stable_Diffusion_UI"
        mc:Ignorable="d"
        d:DesignHeight="250" d:DesignWidth="800" Background="#FF282A31" WindowStyle="None" ResizeMode="NoResize" MaxHeight="250" MaxWidth="800">
    <Grid>
        <Image Height="225" VerticalAlignment="Top" Source="/bg.jpg" Stretch="UniformToFill"/>
        <Label Content="Stable Diffusion UI v2" HorizontalAlignment="Center" VerticalAlignment="Center" Foreground="White" FontSize="48" FontFamily="Helvetica Neue LT Pro">
            <Label.Effect>
                <DropShadowEffect/>
            </Label.Effect>
        </Label>
        <Label x:Name="lbStatus" Content="Loading . . . ." HorizontalAlignment="Center" Width="800" Height="25" VerticalAlignment="Top" Foreground="White" Margin="0,225,0,0"/>

    </Grid>
</Window>
