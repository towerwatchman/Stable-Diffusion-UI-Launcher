<Window x:Class="Stable_Diffusion_UI.UI_Console"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Stable_Diffusion_UI"
        mc:Ignorable="d"
        Title="Stable Diffusion UI Console" Height="450" Width="800" Background="Black" Closing="Window_Closing">
    <Grid>
        <RichTextBox x:Name="uiConsole" Margin="10,10,10,10" Foreground="White" BorderBrush="Black" Background="Black" SelectionBrush="{x:Null}" IsEnabled="False">
            <RichTextBox.Resources>
                <Style TargetType="{x:Type Paragraph}">
                    <Setter Property="Margin" Value="0"/>
                </Style>
            </RichTextBox.Resources>
            <FlowDocument>
                <Paragraph>
                    <Run Text=""/>
                </Paragraph>
            </FlowDocument>
        </RichTextBox>
    </Grid>
</Window>
